            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.taxxib.enterprise.ui.activity.main.MainActivity" />
        </activity>
        <activity
            android:name="com.taxxib.enterprise.ui.activity.FaqActivity"
            android:label="@string/faq_support"
            android:parentActivityName="com.taxxib.enterprise.ui.activity.main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.taxxib.enterprise.ui.activity.TermsActivity"
            android:label="@string/terms_and_conditions"
            android:parentActivityName="com.taxxib.enterprise.ui.activity.main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.taxxib.enterprise.ui.activity.PrivacyActivity"
            android:label="@string/privacy_policy"
            android:parentActivityName="com.taxxib.enterprise.ui.activity.main.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.taxxib.enterprise.ui.activity.past_trip_detail.PastTripDetailActivity"
            android:label="@string/past_trip_details"
            android:parentActivityName="com.taxxib.enterprise.ui.activity.main.MainActivity"
